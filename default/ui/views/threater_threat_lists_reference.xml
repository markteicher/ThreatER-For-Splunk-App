<dashboard version="1.1">
  <label>Threat Intelligence Lists</label>

  <row>
    <panel>
      <title>Threat Intelligence Providers</title>
      <table>
        <search>
          <query>
| inputlookup threater_threat_lists.csv
          </query>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>

  <row>
    <panel>
      <title>Threat Categories</title>
      <table>
        <search>
          <query>
| inputlookup threater_threat_categories.csv
          </query>
        </search>
        <option name="wrap">true</option>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>

</dashboard>
